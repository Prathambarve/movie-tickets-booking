{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "rules": {
    "no-process-exit": "off",
    "strict": [
      2,
      "global"
    ]
  }
}
